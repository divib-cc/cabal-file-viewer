{
  "name": "cabal-file-viewer",
  "displayName": "cabal-file-viewer",
  "description": "Cabal文件查看器 | Viewer for Cabal files (.ebm, .ech格式)",
  "version": "0.0.1",
  "publisher": "divib",
  "icon": "out/icon.png",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "cabal",
    "ebm",
    "ech"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "cabal-file-viewer.cabalViewer",
        "displayName": "Cabal File Viewer",
        "selector": [
          {
            "filenamePattern": "*.ebm"
          },
          {
            "filenamePattern": "*.ech"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}